   ],
   "source": [
    "# Group by (subreddit, author) pair\n",
    "author_counts = messages.groupBy(\"subreddit\", \"author\").count()\n",
    "# Partition by subreddit and order author frequencies\n",
    "window = Window.partitionBy(author_counts['subreddit']).orderBy(author_counts['count'].desc())\n",
    "# Take the first one in the ordered column as the most active user\n",
    "author_counts.select('*', row_number().over(window).alias('rank'))\\\n",
    "                            .filter(col('rank') == 1)\\\n",
    "                            .drop('rank').show()"
   ]
  },
