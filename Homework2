    "frame = {'user':(similarities_user_df['nba']).drop('nba'),'words':(similarities_words_50000_df['nba']).drop('nba')}\n",
    "df = pd.DataFrame(frame)\n",
    "df['subreddit'] = df.index\n",
    "df = df.sort_values(by='words')"
   ]
