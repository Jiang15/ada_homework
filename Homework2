    "rate_mapping={'High':1.0,'Medium':0.0,'Low':-1.0}\n",
    "data_feature_droped[['Attacking Rate','Defending Rate']]=data_feature_droped['Work Rate'] \\\n",
    "                                .str.split('/',expand=True).replace(to_replace=rate_mapping, regex=True)\n",
    "data_feature_droped.drop('Work Rate',axis=1,inplace=True)\n",
    "data_feature_droped.head()"
   ]
